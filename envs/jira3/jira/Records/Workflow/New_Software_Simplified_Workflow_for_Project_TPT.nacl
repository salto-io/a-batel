    Start_work__From__To_Do__Directed@sfffssff = {
      name = "Start work"
      description = ""
      from = [
        {
          id = jira.Status.instance.to_do@s
        },
      ]
      to = jira.Status.instance.in_progress@s
      type = "directed"
